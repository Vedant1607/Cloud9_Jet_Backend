{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^generated/(.*)\\.js$": "<rootDir>/../generated/$1",
    "^generated/(.*)$": "<rootDir>/../generated/$1",
    "^src/(.*)\\.js$": "<rootDir>/../src/$1",
    "^src/(.*)$": "<rootDir>/../src/$1",
    "^(\\.\\.?/.+)\\.js$": "$1"
  }
}
